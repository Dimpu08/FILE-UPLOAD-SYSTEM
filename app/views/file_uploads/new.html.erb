<h1>Upload New File</h1>
<%= link_to 'Back to Files', file_uploads_path, class: 'btn btn-secondary' %>
<%= form_with model: @file_upload, local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>

  <div>
    <%= form.label :file %>
    <%= form.file_field :file %>
  </div>

  <div>
    <%= form.submit 'Upload', class: 'btn btn-primary' %>
  </div>
<% end %>